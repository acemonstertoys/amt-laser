= javascript_include_tag "https://cdn.datatables.net/1.10.10/js/jquery.dataTables.min.js"
= javascript_include_tag "https://cdn.datatables.net/buttons/1.1.1/js/dataTables.buttons.min.js"
= javascript_include_tag "//cdn.datatables.net/buttons/1.1.1/js/buttons.flash.min.js"
= javascript_include_tag "//cdnjs.cloudflare.com/ajax/libs/jszip/2.5.0/jszip.min.js"
= javascript_include_tag "//cdn.rawgit.com/bpampuch/pdfmake/0.1.18/build/pdfmake.min.js"
= javascript_include_tag "//cdn.rawgit.com/bpampuch/pdfmake/0.1.18/build/vfs_fonts.js"
= javascript_include_tag "//cdn.datatables.net/buttons/1.1.1/js/buttons.html5.min.js"
= javascript_include_tag "//cdn.datatables.net/buttons/1.1.1/js/buttons.print.min.js"

.well
  %h3 Entries
  %table.table.table-bordered.datatable
    %thead
      %tr
        %th Entry date
        - if current_user && current_user.is_admin?
          %th "Email"
        %th Start
        %th End
        %th Notes
    %tbody
      = render @entries
